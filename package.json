    "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@types/node": "^20.2.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  }
}
